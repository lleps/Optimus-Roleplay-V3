stock EsFechaMayor(fecha1[], fecha2[])
{
	new
	    Day[2],
	    Month[2],
	    Age[2];

	if(!strcmp(fecha1, fecha2))
	    return 0;

	if(!sscanf(fecha1, "p</>ddd", Day[0], Month[0], Age[0]) && !sscanf(fecha2, "p</>ddd", Day[1], Month[1], Age[1]))
	{
	    if(Age[0] > Age[1])
	        return 1;

		else if(Age[0] < Age[1])
		    return 0;

		if(Month[0] > Month[1])
		    return 1;

		else if(Month[0] < Month[1])
		    return 0;

		if(Day[0] > Day[1] || Day[0] == Day[1])
		    return 1;
	}
	return 0;
}

