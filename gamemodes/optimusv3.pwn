					if (GetPlayerMoney (i) >= PRECIO_MUERTE)
					{
						QuitarDinero (i, PRECIO_MUERTE);
					}
					else
					{
						if (GetPlayerMoney (i) > 0)
						{
					    	GivePlayerMoney (i, -GetPlayerMoney (i));
						}
					}
				}
				else
				{
					HealthData [i] += 1.0;
					SetPlayerHealth (i, HealthData[i]);
