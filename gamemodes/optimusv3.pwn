				RemoveObject(playerid);
				GiveObject(playerid, GOAT(INV_BIDON));
				ApplyAnimation(playerid, "CARRY", "putdwn", 4.0, 0, 1, 1, 0, 0);

			    new
			        Float:x, Float:y, Float:z;

