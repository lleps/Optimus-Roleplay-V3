	CMD:conquistar(playerid,params[])
	{
	    if(IsPlayerInRangeOfPoint(playerid, 3.0, CORDS_LAB_BIGFOOT_EXT))
	    {
	        new
	            Leader = InfoJugador[playerid][jLiderFam];

