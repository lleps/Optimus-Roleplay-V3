stock LoadTable (const tablename[], const header[], const array[][], size)
{
	new
	    str [150],
		i = -1;

	strcat (str, header);
	opmysql_tquery(str, "OnQueryFinish", "ii", INVALID_PLAYER_ID, query_type_unknown);

	size --;
	while (i++ < size)
	{
		A_Format (str, "ALTER TABLE `%s` ADD %s", tablename, array [i]);
		opmysql_tquery(str, "OnQueryFinish", "ii", INVALID_PLAYER_ID, query_type_unknown);
	}
	return 1;
}

