				{
				    if (PedidoCamioneros[i][p_PlayerID] == playerid)
				    {
				        pedido = i;
						break;
					}
				}

				if (pedido == -1)
				    return Error(playerid, "~r~Error: Pedido no encontrado.");

				new
				    NegID = PedidoCamioneros[pedido][p_NegID];

				for (new i; i < MAX_STOCK_NEGOCIO; i++)
				{
