		TextoES[130];

    Loop(i,strlen(text),0)
    {
        switch(text[i])
        {
            case 'Á', 'á': TextoES[i] = '˜';
            case 'É', 'é': TextoES[i] = 'ž';
            case 'Í', 'í': TextoES[i] = '¢';
            case 'Ó', 'ó': TextoES[i] = '¦';
            case 'Ú', 'ú': TextoES[i] = 'ª';
            case 'Ñ', 'ñ': TextoES[i] = '®';
            case '¿': TextoES[i] = '¯';
            case '¡': TextoES[i] = '^';
            default: TextoES[i] = text[i];
        }
    }
    return TextoES;
}

