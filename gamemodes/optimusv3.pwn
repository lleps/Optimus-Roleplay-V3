			InvLoop(i,playerid)
			{
			    if(ItemInventario[playerid][i][invObject] == obj_encendedor)
			    {
				    suc = true;

				    if(ItemInventario[playerid][i][invData] < 8)
				        return Error(playerid, "El ~b~encendedor ~w~no tiene más gas.");

				    ItemInventario[playerid][i][invData] -= 8;
