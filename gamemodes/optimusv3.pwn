	    {
	        if(EstaEnFabricaBlackAso(playerid))
	        {
		        new
		            h = HornoMasCercano(playerid, 1),
		            obj = GetObject(playerid);

				if(h == -1)
				    return Error(playerid, "No estás cerca de un horno.");

				if(InfoHornoBlackAso[h][hornoObj])
