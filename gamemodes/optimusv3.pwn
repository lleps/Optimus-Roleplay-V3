				}
			}
		}
	}

	ActualizarHornosBigFoot();
	ActualizarHornosBlackAso();

	foreach(Player, i)
	{
	    if(PreventedOPU[i])
	    {
	        PreventedOPU[i] --;
		}
	    if(LoginTime[i])
	    {
	        //Prevent unfreeze
	        if (Freezed [i])
	        {
	            if (Freezed [i] == 0xFFFF)
	            {
	                TogglePlayerControllable(i, false);
				}
				else
				{
				    Freezed [i] --;
				    if (Freezed [i] == 1)
				    {
						Descongelar (i);
					}
					else
					{
					    TogglePlayerControllable(i, false);
					}
				}
			}

			if (TaxiTime[i])
			{
			    new
			        DriverID = Taxi_DriverID[i];

