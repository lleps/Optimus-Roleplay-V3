stock SpecSync(playerid, type)
{
	if(type == 0) //Onfoot
	{
	    foreach(Player, i)
	    {
	        if (Specteando_ID[i] == playerid)
	        {
	        	PlayerSpectatePlayer(i, playerid);
			}
		}
	}
	else if(type == 1) //Vehicle
	{
	    foreach(Player, i)
	    {
	        if (Specteando_ID[i] == playerid)
	        {
	        	PlayerSpectateVehicle(i, GetPlayerVehicleID(playerid));
			}
		}
	}
	return 1;
}

