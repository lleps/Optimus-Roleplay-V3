timer Timer_Segundo[1000]()
{
	ownVeh_processSecond();
	MySQL_CheckConnection ();
	#if defined FILE_DEBUG_MODE
	new
	    start_gtc = GetTickCount();
	#endif
	new
	    time_passed = gettime();

