					DeletePVar(playerid, "OFERTA_POR"     );
	    			DeletePVar(playerid, "OFERTA_PRECIO"  );
			        DeletePVar(playerid, "OFERTA_HOUSEID" );
		        	DeletePVar(ID, "OFERTA_ME");
