			    {
					foreach(Player, p)
					    if(GetPVarInt(p, "MINERO_MIN_ID") == i)
					        return Error(playerid, "¡Alguien más está picando este mineral!");

			        SetPlayerFacingAngle(playerid, 180.0+floatabs(360.0-atan2(x-PosMinerales[i][0], y-PosMinerales[i][1])));

