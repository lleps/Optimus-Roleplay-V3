	{
	    GangZoneDestroy(a);
	}


	for(new a; a < MAX_3DTEXT_GLOBAL; a++)
	{
		Delete3DTextLabel(Text3D:a);
	}

	for(new a; a < MAX_PICKUPS; a++)
	{
